{
    "plugins": [],
    "source": {
        "include": ["src"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules|.vscode|scripts|docs)"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "recurse": true,
        "destination": "./docs",
        "template": "node_modules/minami",
        "tutorials": "./tutorials",
        "readme": "./README.md"
    }
}